{
  "name": "@vega-ui/reanimated-icons",
  "version": "0.5.3",
  "description": "Vega UI - Reanimated Icons (React Native)",
  "license": "MIT",
  "author": "VegaUI Team",
  "keywords": [
    "ui-kit",
    "ui",
    "ts",
    "typescript",
    "js"
  ],
  "scripts": {
    "build:icons": "tsx node_modules/@svgr/cli/dist/index src/assets/icons --out-dir src/components --template scripts/svgrTemplate.ts",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf node_modules"
  },
  "type": "module",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "default": "./build/index.js"
    }
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@vega-ui/reanimated-lucide": "^1.0.4"
  },
  "devDependencies": {
    "@react-native/typescript-config": "^0.80.0",
    "@svgr/cli": "^8.1.0",
    "@types/react": "^19.1.8",
    "@vega-ui/svgr-reanimated": "1.0.6",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-native": "^0.80.0",
    "react-native-reanimated": "^3.17.5",
    "react-native-svg": "^15.12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "build/*"
  ]
}