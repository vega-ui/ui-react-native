{
  "name": "@vega-ui/react-native",
  "version": "0.1.0",
  "description": "Vega UI - React Native",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "clean": "rm -rf node_modules"
  },
  "type": "module",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "private": true,
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "default": "./build/index.js"
    }
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@vega-ui/theme": "workspace:*",
    "@vega-ui/utils": "1.2.2"
  },
  "devDependencies": {
    "@react-native/babel-preset": "0.79.3",
    "@react-native/typescript-config": "^0.79.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.13",
    "@types/react": "19.0.0",
    "jest": "^29.6.3",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-test-renderer": "19.0.0",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "sideEffects": [
    "build/*"
  ]
}