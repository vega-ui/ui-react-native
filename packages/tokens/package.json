{
  "name": "@vega-ui/tokens",
  "version": "0.1.1",
  "description": "Vega UI - Tokens",
  "license": "MIT",
  "author": "VegaUI Team",
  "keywords": ["ui-kit", "ui", "ts", "typescript", "js"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf node_modules"
  },
  "type": "module",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "default": "./build/index.js"
    }
  },
  "files": [
    "build"
  ],
  "devDependencies": {
    "tsup": "^8.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "build/*"
  ]
}